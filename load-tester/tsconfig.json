{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "strict": true,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
  }
}
