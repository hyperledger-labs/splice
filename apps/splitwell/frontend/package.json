    "@tanstack/react-query": "4.29.5",
    "@lfdecentralizedtrust/splice-common-frontend": "0.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "6.8.1",
