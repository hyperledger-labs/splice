{
  "name": "splice-pulumi-common-validator",
  "version": "1.0.0",
  "main": "src/index.ts",
  "dependencies": {
    "splice-pulumi-common": "1.0.0"
  },
  "scripts": {
    "fix": "npm run format:fix && npm run lint:fix",
    "check": "npm run format:check && npm run lint:check && npm run type:check",
    "type:check": "tsc --noEmit",
    "format:fix": "prettier --write -- src/**/*.ts",
    "format:check": "prettier --check -- src/**/*.ts",
    "lint:fix": "eslint --fix --max-warnings=0 -- src/**/*.ts",
    "lint:check": "eslint --max-warnings=0 -- src/**/*.ts"
  }
}
